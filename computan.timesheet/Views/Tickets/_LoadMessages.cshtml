@model IEnumerable<computan.timesheet.core.TicketItem>
@if (Model != null && Model.Count() > 0)
{
    foreach (var items in Model)
    {
        <div class="panel panel-flat">
            <div class="panel-heading mt-5">
                <h5 class="panel-title">
                    <small class="display-block" id="modellastmodifiedtime">
                        <b>Date: </b>@items.lastmodifiedtime
                    </small>
                    <small class="display-block" id="modelfrom">
                        <b>From: </b>@items.from
                    </small>
                    <small class="display-block" id="modeldisplayto">
                        <b>To: </b>@items.displayto
                    </small>
                    <small class="display-block" id="modeldisplaycc">
                        <b>Cc: </b>@items.displaycc
                    </small>
                </h5>
                <a class="heading-elements-toggle">
                    <i class="icon-menu"></i>
                </a>
            </div>
            <div class="panel-body" id="modelemailbody"> @Html.Raw(items.uniquebody) </div>
            <div class="panel-footer"> </div>
        </div>
    }
}